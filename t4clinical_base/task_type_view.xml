<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
            <record model="ir.ui.view" id="view_t4_clinical_task_type_search">
                <field name="name">T4 Clinical Task Data Type Search View</field>
                <field name="model">t4.clinical.task.type</field>
                <field name="arch" type="xml">
                    <search string="T4 Clinical Task Data Type Search View">
                        <group expand="0" string="Group By...">
                          <field name="data_model" string="Data Model" context="{'group_by':'data_model'}"/>
                        </group>
                    </search>
                </field>
            </record>
            <record model="ir.ui.view" id="view_t4_clinical_task_type_tree">
                <field name="name">T4 Clinical Task Data Type Tree View</field>
                <field name="model">t4.clinical.task.type</field>
                <field name="arch" type="xml">
                    <tree string="T4 Clinical Task Data Type Tree View">
                       <field name="data_model" />
                       <field name="act_window_xmlid" />
                       <field name="act_window_id" />
                    </tree>
                </field>
            </record>    
            <record model="ir.ui.view" id="view_t4_clinical_task_type_form">
                <field name="name">T4 Clinical Task Data Type Form View</field>
                <field name="model">t4.clinical.task.type</field>
                <field name="arch" type="xml">
                    <form string="T4 Clinical Task Data Type Form View">
                       <field name="data_model" />
                       <field name="act_window_xmlid" />
                       <field name="act_window_id" />
                    </form>
                </field>
            </record>
            <record model="ir.actions.act_window" id="action_t4_clinical_task_type" >
                <field name="name">T4 Clinical Task Data Type Window Action</field>
                <field name="res_model">t4.clinical.task.type</field>
                <field name="view_type">form</field>
                <field name="view_mode">tree,form</field>
                <field name="domain"></field>
                <field name="search_view_id" ref="view_t4_clinical_task_type_search"/>
            </record>
            <record model="ir.actions.act_window.view" id="act_view_t4_clinical_task_type_tree">
                <field eval="0" name="sequence"/>
                <field name="view_mode">tree</field>
                <field name="view_id" ref="view_t4_clinical_task_type_tree"/>
                <field name="act_window_id" ref="action_t4_clinical_task_type"/>
            </record>
            <record model="ir.actions.act_window.view" id="act_view_t4_clinical_task_type_form">
                <field eval="1" name="sequence"/>
                <field name="view_mode">form</field>
                <field name="view_id" ref="view_t4_clinical_task_type_form"/>
                <field name="act_window_id" ref="action_t4_clinical_task_type"/>
            </record>
     </data>
</openerp>