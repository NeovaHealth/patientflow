<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>	
    <!-- Types -->
		<record model="t4.clinical.activity.type" id="patient_move_data_type">
			<field name="summary">Patient Move</field>
			<field name="data_model">t4.clinical.patient.move</field>
 			<field name="start_view_id" ref="view_patient_move_form" />
 			<field name="schedule_view_id" ref="view_patient_move_form" />
 			<field name="submit_view_id" ref="view_patient_move_form" />
 			<field name="complete_view_id" ref="view_patient_move_form" />
 			<field name="cancel_view_id" ref="view_patient_move_form" />
 			
		</record>
        <record model="t4.clinical.activity.type" id="hca_notification_data_type">
			<field name="summary">HCA Notification</field>
			<field name="data_model">t4.clinical.notification.hca</field>
		</record>
        <record model="t4.clinical.activity.type" id="nurse_notification_data_type">
			<field name="summary">Nurse Notification</field>
			<field name="data_model">t4.clinical.notification.nurse</field>
		</record>
		<record model="t4.clinical.activity.type" id="patient_placement_data_type">
			<field name="summary">Patient Placement</field>
			<field name="data_model">t4.clinical.patient.placement</field>
			<field name="complete_view_id" ref="view_patient_placement_complete" />
		</record>			
		<record model="t4.clinical.activity.type" id="patient_admission_data_type">
			<field name="summary">Patient Admission</field>
			<field name="data_model">t4.clinical.patient.admission</field>
		</record>
		<record model="t4.clinical.activity.type" id="patient_discharge_data_type">
			<field name="summary">Patient Discharge</field>
			<field name="data_model">t4.clinical.patient.discharge</field>
		</record>
		<record model="t4.clinical.activity.type" id="observation_height_weight_data_type">
			<field name="summary">Observation Height, Weight</field>
			<field name="data_model">t4.clinical.patient.observation.height_weight</field>
			<field name="submit_view_id" ref="view_observation_height_weight" />
		</record>		
		<record model="t4.clinical.activity.type" id="spell_data_type">
			<field name="summary">Spell of Care</field>
			<field name="data_model">t4.clinical.spell</field>

		</record>			
		<record model="t4.clinical.activity.type" id="adt_patient_register_data_type">
			<field name="summary">ADT Patient Register</field>
			<field name="data_model">t4.clinical.adt.patient.register</field>

		</record>
		<record model="t4.clinical.activity.type" id="adt_patient_admit_data_type">
			<field name="summary">ADT Patient Admit</field>
			<field name="data_model">t4.clinical.adt.patient.admit</field>

		</record>
		<record model="t4.clinical.activity.type" id="adt_patient_transfer_data_type">
			<field name="summary">ADT Patient Transfer</field>
			<field name="data_model">t4.clinical.adt.patient.transfer</field>

		</record>
		<record model="t4.clinical.activity.type" id="adt_patient_merge_data_type">
			<field name="summary">ADT Patient Merge</field>
			<field name="data_model">t4.clinical.adt.patient.merge</field>

		</record>			
		<record model="t4.clinical.activity.type" id="observation_ews_data_type">
			<field name="summary">Observation EWS</field>
			<field name="data_model">t4.clinical.patient.observation.ews</field>
		</record>
        <record model="t4.clinical.activity.type" id="observation_gcs_data_type">
			<field name="summary">Observation GCS</field>
			<field name="data_model">t4.clinical.patient.observation.gcs</field>
		</record>
        <record model="t4.clinical.activity.type" id="observation_vips_data_type">
			<field name="summary">Observation VIPS</field>
			<field name="data_model">t4.clinical.patient.observation.vips</field>
		</record>
		<record model="t4.clinical.activity.type" id="device_connect_data_type">
			<field name="summary">Device Connect</field>
			<field name="data_model">t4.clinical.device.connect</field>
		</record>	
		<record model="t4.clinical.activity.type" id="device_disconnect_data_type">
			<field name="summary">Device Disconnect</field>
			<field name="data_model">t4.clinical.device.disconnect</field>
		</record>	
		<record model="t4.clinical.activity.type" id="patient_o2target">
			<field name="summary">Patient O2 Target</field>
			<field name="data_model">t4.clinical.patient.o2target</field>
		</record>			
<!-- Sequences -->
        <record id="spell_sequence_type" model="ir.sequence.type">
            <field name="name">Spell</field>
            <field eval="'t4.clinical.spell'" name="code"/>
        </record>	
        <record id="spell_sequence" model="ir.sequence">
            <field name="name">Spell</field>
            <field eval="6" name="padding"/>
            <field eval="'t4.clinical.spell'" name="code"/>
            <field name="prefix">SPELL/%(year)s/</field>
        </record>													
	</data>
</openerp>